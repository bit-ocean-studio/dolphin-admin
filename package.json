    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cspell": "^7.3.8",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "naive-ui": "^2.35.0",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vfonts": "^0.0.3",
