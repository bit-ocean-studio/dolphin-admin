    "@vueuse/motion": "^2.0.0",
    "axios": "^1.6.0",
    "d3": "^7.8.5",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "socket.io-client": "^4.7.2",
    "vue": "^3.3.8",
    "vue-i18n": "9.4.1",
    "vue-router": "^4.2.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@brucesong/eslint-config-vue": "^1.0.16",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@dolphin-admin/auto-import": "^0.0.8",
    "@dolphin-admin/bootstrap-animation": "^0.0.2",
    "@iconify/json": "^2.2.138",
    "@iconify/vue": "^4.1.1",
    "@tauri-apps/cli": "^1.5.6",
