{
  "private": true,
  "name": "dolphin-admin-web",
  "version": "0.0.1",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type:check && pnpm build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "dev:desktop": "cargo tauri dev",
    "prepare": "husky install",
    "cspell:check": "npx cspell '**'",
    "cspell:sort": "node ./scripts/cspell-sort.js",
    "eslint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type:check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "commit": "npx git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "nprogress": "0.2.0",
    "pinia": "^2.1.4",
    "qrcode": "^1.5.3",
    "vue": "^3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@iconify/json": "^2.2.82",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.3.1",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^3.0.0",
    "cspell": "^6.31.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-define-config": "^1.21.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "naive-ui": "^2.34.4",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.3",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vfonts": "^0.0.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.2.0"
  },
  "license": "MIT"
}
